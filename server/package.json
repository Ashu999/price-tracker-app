{
  "name": "price-tracker-app",
  "version": "1.0.0",
  "description": "Get Notification on Item's Price change",
  "main": "index.ts",
  "scripts": {
    "start": "dotenv -e .env.local nodemon --watch './**/*.ts' --exec 'ts-node' index.ts",
    "prisma-migrate": "npx prisma migrate deploy",
    "prisma-generate": "npx prisma generate",
    "build": "tsc",
    "start:prod": "node dist/index.js",
    "start:migrate:prod": "npx prisma migrate deploy && node dist/index.js",
    "start:baseline:migrate:prod": "npx prisma migrate resolve --applied 20221124154712_init && npx prisma migrate deploy && node dist/index.js",
    "start:generate:prod": "npx prisma generate && node dist/index.js",
    "start:migrate:generate:prod": "npx prisma migrate deploy && npx prisma generate && node dist/index.js"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "author": "Ashutosh Sharma",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^2.1.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "module-alias": "^2.2.2",
    "nodemailer": "^6.8.0",
    "prisma": "^4.8.1",
    "puppeteer": "^18.1.0",
    "supertokens-node": "^12.0.5"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.0",
    "@types/nodemailer": "^6.4.7",
    "@types/puppeteer": "^5.4.6",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1"
  }
}
